<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShapeImageView">
        <attr name="shape" format="enum">
            <enum name="rect" value="1"/>
            <enum name="circle" value="2"/>
            <enum name="oval" value="3"/>
        </attr>
        <attr name="shape_round_radius" format="dimension"/>
        <attr name="shape_border_size" format="dimension"/>
        <attr name="shape_border_color" format="color"/>
    </declare-styleable>
</resources>